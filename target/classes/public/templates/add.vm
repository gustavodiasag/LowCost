    <h1> Cadastrar serviço </h1>
    <form onsubmit="" action="/add/" method="post">
      <label for="empresa">Empresa que ofereçe o serviço</label>
      <br>
      <input class = "area"
        required
        name="company"
        id="empresa"
        placeholder="Netflix, Claro, VIVO, CEMIG..."
      />
		<br>
      <label for="servico">Serviço</label>
      <br>
      <input class = "area"
        required
        name="description"
        id="servico"
        placeholder="Internet, plano de TV, água, luz..."
      />
		<br>
      <label for="preco">Preço</label>
      <br>
      <input class = "area"
        required
        name="price"
        id="preco"
        placeholder="Mensal: 30.00, 49.99..."
      />
		<br>
      <label for="notas">Observações</label>
      <br>
      <textarea class = "area maior" 
        name="comment"
        id="notas"
        placeholder="Liguei pra o número deles e depois de reclamar bastante deixaram eu diminuir pra esse preço sem perder nada."
      ></textarea>
      <br>
      
		<button class="btn botao-padrao" type="submit" id="btn-forum">Enviar</button>
    </form>
  </main>
